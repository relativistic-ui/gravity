{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "dev:prepare",
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".nuxt/**",
        "dist/**",
        ".output/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "^lint:fix"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:prepare": {
      "dependsOn": [
        "^dev:prepare"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "coverage": {
      "dependsOn": [
        "^coverage"
      ]
    },
    "testedBuild": {
      "dependsOn": [
        "test",
        "build"
      ]
    },
    "deploy:patch": {
      "dependsOn": [
        "testedBuild",
        "^deploy:patch"
      ]
    },
    "deploy:minor": {
      "dependsOn": [
        "testedBuild",
        "^deploy:minor"
      ]
    },
    "deploy:major": {
      "dependsOn": [
        "testedBuild",
        "^deploy:major"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    }
  }
}