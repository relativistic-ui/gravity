{
  "name": "@relativistic-ui/gravity-vue",
  "version": "0.0.5",
  "type": "module",
  "main": "./dist/index.js",
  "require": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "private": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "deploy:patch": "yarn version --patch && yarn publish --access public",
    "deploy:minor": "yarn version --minor && yarn publish --access public",
    "deploy:major": "yarn version --major && yarn publish --access public"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@relativistic-ui/gravity-core": "*",
    "@relativistic-ui/typescript-config": "*",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/test-utils": "^2.4.3",
    "happy-dom": "^12.10.3",
    "tsup": "^6.0.1",
    "typescript": "^4.7.4",
    "vitest": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
